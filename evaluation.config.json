{
  "runtime": {
    "targets": ["node", "deno", "bun"],
    "browserTarget": false
  },
  "render": {
    "suiteName": "render-v3",
    "widths": [60, 80, 100, 120],
    "holdoutMod": 10,
    "profiles": {
      "ci": {
        "includeHoldout": true,
        "minExecutedFraction": 0.9,
        "minCases": 250,
        "requireRuntimeMatrix": true
      },
      "release": {
        "includeHoldout": true,
        "minExecutedFraction": 1,
        "minCases": 1000,
        "requireRuntimeMatrix": true
      }
    },
    "metrics": {
      "textTokenF1": 0.97,
      "linkLabelF1": 0.995,
      "tableMatrixF1": 0.95,
      "preWhitespaceExact": 0.995,
      "outlineF1": 0.98
    },
    "comparativeWinDelta": 0.005
  },
  "baselines": {
    "engines": [
      {
        "name": "lynx",
        "version": "2.9.2dev.1"
      },
      {
        "name": "w3m",
        "version": "0.5.3+git20230121"
      },
      {
        "name": "links2",
        "version": "2.30"
      }
    ]
  },
  "oracleSupplyChain": {
    "maxOraclePackageCount": 110,
    "requiredRootPackages": ["lynx", "w3m", "links2"],
    "provenancePolicy": {
      "requireSnapshotReplayMode": true,
      "requireHttpsSnapshotRoot": true,
      "requireSnapshotId": true,
      "requireKeyringPath": true,
      "requireReleaseMetadata": true,
      "requireSignatureKeyHex": true,
      "rejectUnknownSignatureKey": true,
      "requirePackageIndexes": true,
      "requireHttpsReleaseUrls": true,
      "requireHttpsIndexUrls": true,
      "requireIndexSha256": true,
      "requireHttpsPackageDownloadUrls": true
    }
  },
  "benchmarks": {
    "required": ["render-width-80", "render-width-120"],
    "minSampleCases": 300
  },
  "fuzz": {
    "profiles": {
      "ci": {
        "seed": 20260226,
        "caseCount": 128,
        "maxDepth": 5,
        "sectionCount": 8,
        "topSlowest": 10
      },
      "release": {
        "seed": 20260226,
        "caseCount": 512,
        "maxDepth": 6,
        "sectionCount": 10,
        "topSlowest": 20
      }
    }
  }
}
