ul type=\"A\" class=\"type-A\"></ul>\n  <li type=\"A\" class=\"type-A\"></li>\n  <ol type=\"i\" class=\"type-i\"></ol>\n  <ul type=\"i\" class=\"type-i\"></ul>\n  <li type=\"i\" class=\"type-i\"></li>\n  <ol type=\"I\" class=\"type-I\"></ol>\n  <ul type=\"I\" class=\"type-I\"></ul>\n  <li type=\"I\" class=\"type-I\"></li>\n  <ol type=\"none\"></ol>\n  <ul type=\"none\"></ul>\n  <li type=\"none\"></li>\n  <ol type=\"NONE\"></ol>\n  <ul type=\"NONE\"></ul>\n  <li type=\"NONE\"></li>\n  <ol type=\"disc\"></ol>\n  <ul type=\"disc\"></ul>\n  <li type=\"disc\"></li>\n  <ol type=\"DISC\"></ol>\n  <ul type=\"DISC\"></ul>\n  <li type=\"DISC\"></li>\n  <ol type=\"circle\"></ol>\n  <ul type=\"circle\"></ul>\n  <li type=\"circle\"></li>\n  <ol type=\"CIRCLE\"></ol>\n  <ul type=\"CIRCLE\"></ul>\n  <li type=\"CIRCLE\"></li>\n  <ol type=\"square\"></ol>\n  <ul type=\"square\"></ul>\n  <li type=\"square\"></li>\n  <ol type=\"SQUARE\"></ol>\n  <ul type=\"SQUARE\"></ul>\n  <li type=\"SQUARE\"></li>\n\n  <ol>\n    <li value=\"10\"></li>\n    <li value=\"10xyz\"></li>\n    <li value=\"10e10\"></li>\n    <li value=\"xyz\"></li>\n  </ol>\n\n  <ol start=\"10\"><li></li></ol>\n  <ol start=\"10xyz\"><li></li></ol>\n  <ol start=\"10e10\"><li></li></ol>\n  <ol start=\"xyz\"><li></li></ol>\n  <ol reversed><li></li></ol>\n  <ol reversed start=\"20\"><li></li></ol>\n  <ol reversed start=\"20xyz\"><li></li></ol>\n  <ol reversed start=\"20e10\"><li></li></ol>\n  <ol reversed start=\"xyz\"><li></li></ol>\n\n  <ul data-skip dir=\"rtl\"><li dir=\"ltr\"></li></ul>\n  <ul data-skip dir=\"ltr\"><li dir=\"rtl\"></li></ul>\n\n</div>\n\n<div id=\"refs\"></div>\n\n<script>\n  const props = [\n    'display',\n    'margin-top',\n    'margin-right',\n    'margin-bottom',\n    'margin-left',\n    'padding-top',\n    'padding-right',\n    'padding-bottom',\n    'padding-left',\n    'list-style-type',\n    'counter-set',\n    'counter-reset',\n    'counter-increment',\n    'text-align',\n    'list-style-position',\n  ];\n  runUAStyleTests(props);\n\n</script>\n"
